    "def f(y):\n",
    "    return -y\n",
    "N=10000\n",
    "a,b=0.,1.\n",
    "t=linspace(a,b,N)\n",
    "y=empty(N)\n",
    "y[0]=1\n",
    "h=(b-a)/N\n",
    "for i in range(1,N):\n",
    "    y[i]=y[i-1]+h*f(y[i-1])\n",
    "plot(t,y,'.')\n",
    "plot(t,exp(-t))\n",
    "show()\n",
    "    "
   ]
